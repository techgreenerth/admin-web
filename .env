VITE_GOOGLE_ANALYTICS_ID="your-ga-id"
VITE_SENTRY_DSN="your-sentry-dsn"

# Feature Flags
VITE_ENABLE_AI_OVERRIDE="true"
VITE_ENABLE_BLOCKCHAIN="false"
